    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "shadcn": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
